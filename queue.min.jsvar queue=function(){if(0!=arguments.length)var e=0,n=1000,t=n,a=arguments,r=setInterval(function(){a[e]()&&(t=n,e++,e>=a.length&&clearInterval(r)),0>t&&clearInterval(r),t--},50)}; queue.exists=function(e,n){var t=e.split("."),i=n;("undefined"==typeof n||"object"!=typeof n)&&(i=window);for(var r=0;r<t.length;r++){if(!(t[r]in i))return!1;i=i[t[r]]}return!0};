var queue=function(){if(0!=arguments.length)var e=0,n=1000,t=n,a=arguments,r=setInterval(function(){a[e]()&&(t=n,e++,e>=a.length&&clearInterval(r)),0>t&&clearInterval(r),t--},50)};

// optional
queue.exists=function(e,n){var t=e.split("."),i=n;("undefined"==typeof n||"object"!=typeof n)&&(i=window);for(var r=0;r<t.length;r++){if(!(t[r]in i))return!1;i=i[t[r]]}return!0};
